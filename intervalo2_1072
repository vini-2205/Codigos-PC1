#include <stdio.h>

int main(){
  int N, X, contador = 0,in=0,out=0;
  scanf("%d",&N);
  while(contador<N){
    scanf("%d",&X);
    contador = contador + 1;
    if((X>=10)&&(X<=20)){
        in=in+1;
    }
    else{
       out=out+1;
    }
    }
    printf("%d in\n", in);
    printf("%d out\n", out);
    return 0;

}
